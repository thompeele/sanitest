<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="0">
	<record id="external_fulfil_module_category" model="ir.module.category">
		<field name="name">External Fulfilment</field>
		<field name="description">External Fulfilment</field>
		<field name="sequence">10</field>
    </record>
	
	<record id="group_external_fulfil_user" model="res.groups">
		<field name="name">User</field>
		<field name="comment">User Access</field>
		<field name="category_id" ref="external_fulfil_module_category"/>
	</record>

	<record id="group_external_fulfil_manager" model="res.groups">
		<field name="name">Manager</field>
		<field name="comment">Full Access</field>
		<field name="category_id" ref="external_fulfil_module_category"/>
		<field name="users" eval="[(4, ref('base.user_root'))]"/>
	</record>

</odoo>
