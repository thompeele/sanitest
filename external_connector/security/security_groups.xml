<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

	<record id="external_connector_module_category" model="ir.module.category">
		<field name="name">External Connector</field>
		<field name="description">External Connector App</field>
		<field name="sequence">10</field>
    </record>
	
	<record id="group_external_connector_user" model="res.groups">
		<field name="name">User</field>
		<field name="comment">User Access</field>
		<field name="category_id" ref="external_connector_module_category"/>
	</record>

	<record id="group_external_connector_manager" model="res.groups">
		<field name="name">Manager</field>
		<field name="comment">Full Access</field>
		<field name="category_id" ref="external_connector_module_category"/>
		<field name="implied_ids" eval="[(4, ref('external_connector.group_external_connector_user'))]"/>
		<field name="users" eval="[(4, ref('base.user_root'))]"/>
	</record>

</odoo>
