<odoo>
    <data>
        <record id="error_log_cleanup_cron" model="ir.cron">
            <field name="name">Error log cleanup </field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doal">2</field>
            <!--<field name="nextcall" >2016-12-31 23:59:59</field>-->
            <field name="model" eval="'magento.error.log'" />
            <field name="function" eval="'cleanup'" />
            <field name="args" eval="" />
            <!--<field name="priority" eval="5" />-->
        </record>
    </data>
</odoo>